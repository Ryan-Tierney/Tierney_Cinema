<%= form_for(@user) do |f| %> 

    <%= f.label :email %> 
    <%= f.email_field :email, required: true %>: 
    <br>
    <br>
    <%= f.label :username %>
    <%= f.text_field :username, required: true %>: 
    <br>
    <br>
    <%= f.label :age %> 
    <%= f.number_field :age %> 
    <br>
    <br>
    <%= f.label :password %> 
    <%= f.password_field :password, required: true %>: 
    <br>
    <br>
    <%= f.submit "submit" %> 

<% end %> 